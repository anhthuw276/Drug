import pandas as pd

# Đọc tệp Excel
file_path = 'Drugdbcc.xlsx'                       #bạn đặt tên file ntn thì sửa lại như vậy để code tìm 
data = pd.read_excel(file_path)

# Loại bỏ các hàng trùng lặp và giữ lại một hàng
data_cleaned = data.drop_duplicates()

# Ghi kết quả vào tệp Excel mới
output_path = 'Drugdbcc_cleaned.xlsx'                          # đặt tên ntn cũng được
data_cleaned.to_excel(output_path, index=False)

print(f'Dữ liệu đã được xử lý và lưu tại: {output_path}')          #phần này cho phần terminal thôi, ghi ntn cũng được
